{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "chat-work launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/work/chat-work",
      "cwd": "${workspaceFolder}",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Enable debuginfod",
          "text": "set debuginfod enabled on",
          "ignoreFailures": true
        }
      ]
    }
  ]
}